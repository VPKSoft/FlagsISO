<Project Sdk="Eto.Forms/2.7.0">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<OutputType>Library</OutputType>
		<DocumentationFile>bin\$(Configuration)\FlagsISO.EtoForms.xml</DocumentationFile>
		<Version>1.0.0.0</Version>
		<Description>An Eto.Forms package for the FlagISO library.</Description>
		<PackageProjectUrl>https://github.com/VPKSoft/FlagsISO</PackageProjectUrl>
		<PackageIcon>VPKSoftIcon.png</PackageIcon>
		<RepositoryUrl>https://github.com/VPKSoft/FlagsISO</RepositoryUrl>
		<PackageTags>FlagsISO VPKSoft country flags ISO Eto Forms EtoForms</PackageTags>
		<PackageReleaseNotes></PackageReleaseNotes>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Copyright>Copyright Â© VPKSoft 2022</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\VPKSoftIcon.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SkiaSharp.NativeAssets.Linux.NoDependencies" Version="2.88.0" />
		<PackageReference Include="Svg.Skia" Version="0.5.13" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\FlagsISO\FlagsISO.csproj" />
	</ItemGroup>

</Project>
